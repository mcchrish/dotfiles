[dotfiles]
repository = ~/dotfiles
ignore = [
  '.git',
  '.gitignore',
  '.gitmodules',
  '.swp',
  '.DS_Store',
  '.Trash',
  'submodules',
  'iterm2',

  'firefox',
  'karabiner',
  'README.md',
  'LICENSE',
  'osx-tweaks',
  'scripts',
  'config',

  'zpreztorc',
  'term-italic',
  ]

externals = { 'config/nvim/init.vim': '~/dotfiles/config/nvim/init.vim' }

packages = ['zgen']

